{
  "name": "desktop_task_node",
  "productName": "desktop_task_node",
  "version": "1.0.0",
  "description": "Desktop Task Node",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "prepare": "husky install",
    "test:unit-webapp": "jest --selectProjects \"Webapp Unit Tests\""
  },
  "keywords": [],
  "author": {
    "name": "Koii"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "desktop_task_node"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "desktop_task_node"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "name": "desktop_task_node",
            "format": "ULFO"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "name": "desktop_task_node"
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "devContentSecurityPolicy": "connect-src 'self' http://localhost:8899/ https://arweave.net/ https://*.koii.live/",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "preload": {
                    "js": "./src/preload.ts"
                  },
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.64",
    "@electron-forge/maker-deb": "^6.0.0-beta.64",
    "@electron-forge/maker-rpm": "^6.0.0-beta.64",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.64",
    "@electron-forge/maker-zip": "^6.0.0-beta.64",
    "@electron-forge/plugin-webpack": "6.0.0-beta.64",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/base-64": "^1.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/express-http-proxy": "^1.6.3",
    "@types/jest": "^28.1.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/leveldown": "^4.0.3",
    "@types/levelup": "^5.1.1",
    "@types/node-cron": "^3.0.1",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.2",
    "css-loader": "^6.7.1",
    "electron": "^19.0.5",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^29.2.1",
    "lint-staged": ">=13",
    "mini-css-extract-plugin": "^2.6.1",
    "miragejs": "^0.1.45",
    "node-loader": "^2.0.0",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@_koi/kohaku": "^1.1.2",
    "@_koi/sdk": "^2.4.13",
    "@_koi/web3.js": "^0.0.6",
    "@_koii/koii-styleguide": "^0.2.7-alpha",
    "@ebay/nice-modal-react": "^1.2.7",
    "@electron-forge/maker-dmg": "^6.0.0-beta.64",
    "@headlessui/react": "^1.7.5",
    "@solana/buffer-layout": "^4.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/react-syntax-highlighter": "^15.5.2",
    "axios": "^0.27.2",
    "base-64": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "bs58": "^5.0.0",
    "buffer-layout": "^1.2.2",
    "cheerio": "^1.0.0-rc.12",
    "clsx": "^1.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.0",
    "crypto-random-string": "^5.0.0",
    "ed25519-hd-key": "^1.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.1",
    "express-http-proxy": "^1.6.3",
    "jsonwebtoken": "^8.5.1",
    "leveldown": "^6.1.1",
    "levelup": "^5.1.1",
    "lottie-react": "^2.3.1",
    "node-abi": "^3.0.0",
    "node-cron": "^3.0.1",
    "puppeteer": "^15.4.0",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-countup": "^6.4.0",
    "react-dom": "^18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-pin-input": "^1.2.0",
    "react-query": "^3.39.1",
    "react-router-dom": "^6.3.0",
    "react-syntax-highlighter": "^15.5.0",
    "read-last-lines": "^1.8.0",
    "smartweave": "^0.4.49",
    "stream-browserify": "^3.0.0",
    "tailwind-merge": "^1.3.0",
    "tailwindcss": "^3.1.3",
    "tweetnacl": "^1.0.3",
    "use-file-picker": "^1.4.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix"
  }
}
